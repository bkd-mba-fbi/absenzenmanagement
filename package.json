{
  "name": "absenzenmanagement",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "lint:format": "ng lint $(ng config defaultProject) --tslint-config tslint-prettier.json --files \"src/**/*.ts\"",
    "format": "prettier --write \"./**/*.{js,ts,json,css,scss,html,md,yaml}\"",
    "e2e": "ng e2e",
    "preanalyze": "ng build --stats-json",
    "analyze": "npx webpack-bundle-analyzer dist/$(ng config defaultProject)/stats.json",
    "postanalyze": "rm dist/$(ng config defaultProject)/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@ng-bootstrap/ng-bootstrap": "^4.1.3",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "^4.3.1",
    "core-js": "^3.1.2",
    "date-fns": "^1.30.1",
    "io-ts": "^1.8.6",
    "io-ts-reporters": "0.0.21",
    "io-ts-types": "^0.4.7",
    "lodash-es": "^4.17.11",
    "monocle-ts": "^1.7.2",
    "newtype-ts": "^0.2.4",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@types/jasmine": "~3.3.12",
    "@types/jasminewd2": "~2.0.6",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "~12.0.2",
    "codelyzer": "~5.0.1",
    "husky": "^2.3.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "^8.1.7",
    "node-sass": "^4.12.0",
    "prettier": "1.17.1",
    "protractor": "~5.4.2",
    "ts-node": "~8.1.0",
    "tslint": "~5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-jasmine-rules": "^1.6.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "./src/app/**/*.ts": [
        "tslint",
        "prettier --write",
        "git add"
      ],
      "./**/*.{js,json,css,scss,html,md,yaml}": [
        "prettier --write",
        "git add"
      ]
    }
  }
}
